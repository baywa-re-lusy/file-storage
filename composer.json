{
    "name": "baywa-re-lusy/file-storage",
    "description": "BayWa r.e. LUSY File Storage Tools",
    "type": "library",
    "license": "proprietary",
    "homepage": "https://github.com/baywa-re-lusy/file-storage",
    "require": {
        "ext-fileinfo": "*",
        "php": ">= 8.1",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "microsoft/azure-storage-blob": "^1.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5",
        "php-mock/php-mock": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "BayWaReLusy\\FileStorage\\": "src/FileStorage"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BayWaReLusy\\FileStorage\\Test\\": "tests"
        }
    }
}
